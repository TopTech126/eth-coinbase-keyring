{
  "name": "@lux-wallet/eth-coinbase-keyring",
  "version": "0.0.0-beta.8",
  "description": "A keyring that uses the Coinbase Wallet SDK to sign transactions",
  "keywords": [
    "ethereum",
    "keyring"
  ],
  "homepage": "https://github.com/luxfi/eth-coinbase-keyring#readme",
  "bugs": {
    "url": "https://github.com/luxfi/eth-coinbase-keyring/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luxfi/eth-coinbase-keyring.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "patches"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:clean": "rimraf dist && yarn build",
    "build:docs": "typedoc",
    "lint": "yarn lint:eslint && yarn lint:misc --check && yarn lint:dependencies",
    "lint:dependencies": "depcheck",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' '**/*.yml' '!.yarnrc.yml' --ignore-path .gitignore --no-error-on-unmatched-pattern",
    "prepack": "./scripts/prepack.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "example": "vite ."
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "beta",
    "@ethereumjs/util": "^8.1.0",
    "@metamask/eth-sig-util": "^7.0.0",
    "@metamask/utils": "^8.1.0",
    "patch-package": "^8.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@ethereumjs/tx": "^4.1.1",
    "@lavamoat/allow-scripts": "^3.0.0",
    "@metamask/auto-changelog": "^3.1.0",
    "@metamask/eslint-config": "^11.1.0",
    "@metamask/eslint-config-jest": "^11.1.0",
    "@metamask/eslint-config-nodejs": "^11.1.0",
    "@metamask/eslint-config-typescript": "^11.1.0",
    "@peculiar/webcrypto": "^1.4.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/ethereumjs-tx": "^1.0.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.10",
    "@types/randombytes": "^2.0.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ethereumjs-tx": "^1.3.7",
    "events": "^3.3.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.8.7",
    "qrcode.vue": "^3.4.1",
    "ts-jest": "^29.0.5",
    "typedoc": "^0.25.1",
    "typescript": "~4.8.4",
    "vite": "^5.0.2",
    "vue": "^3.3.8"
  },
  "packageManager": "yarn@3.3.0",
  "engines": {
    "node": "^16.20 || ^18.16 || >=20"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "lavamoat": {
    "allowScripts": {
      "keccak": true,
      "secp256k1": true,
      "@lavamoat/preinstall-always-fail": false,
      "ethereumjs-tx>ethereumjs-util>ethereum-cryptography>keccak": false,
      "ethereumjs-tx>ethereumjs-util>ethereum-cryptography>secp256k1": false,
      "@coinbase/wallet-sdk>@solana/web3.js>bigint-buffer": false,
      "@coinbase/wallet-sdk>@solana/web3.js>rpc-websockets>bufferutil": false,
      "@coinbase/wallet-sdk>@solana/web3.js>rpc-websockets>utf-8-validate": false,
      "@coinbase/wallet-sdk>keccak": false
    }
  }
}
